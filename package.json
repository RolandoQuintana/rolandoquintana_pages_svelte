{
	"name": "rolandoquintana-pages-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development vite dev",
		"build": "NODE_ENV=production vite build",
		"deploy": "npm run build && touch docs/.nojekyll",
		"deploy:github": "npm run deploy && git add . && git commit -m \"Deploy to GitHub Pages\" && git push origin main",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"tina": "tinacms dev -c \"npm run dev\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/node": "^22.14.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@tinacms/cli": "^1.9.3",
		"sass": "^1.86.0",
		"scrambling-text": "^1.2.0",
		"tinacms": "^2.7.3"
	}
}
